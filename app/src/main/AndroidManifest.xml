<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.weichen.grocery2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddProductActivity" ></activity>
        <activity android:name=".AddCouponActivity" ></activity>
        <activity android:name=".ListProductsActivity" ></activity>
        <activity android:name=".ListCouponsActivity" ></activity>
        <activity android:name=".ViewCouponActivity" ></activity>
        <activity android:name=".ShoppingListLargestDiscountActivity" ></activity>
        <activity android:name=".FinalPriceActivity" ></activity>
    </application>

</manifest>